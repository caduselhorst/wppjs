generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model wppmessage {
  id        BigInt    @id @default(autoincrement())
  date      DateTime? @default(now()) @db.Timestamp(6)
  contact   String    @db.VarChar
  sendedmsg String
  result    Int
  resultmsg String
}
